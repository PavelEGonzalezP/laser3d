<launch>
    <!-- Start joint controller -->
    <rosparam file="$(find laser3d)/config/motor.yaml" command="load"/>

    <node name="laser_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port motor_port
                laser_controller"
          output="screen"/>
</launch>
