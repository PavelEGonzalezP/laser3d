<launch>
    <!-- Start tilt joint controller -->
    <rosparam file="$(find my_dynamixel_tutorial)/config/tilt.yaml" command="load"/>
    <node name="laser_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port laser_port
                laser_controller"
          output="screen"/>
</launch>
